@font-face 
	font-family: 'gimmefile'
	src:url('fonts/gimmefile.eot')
	src:url('fonts/gimmefile.eot?#iefix') format('embedded-opentype'),
		url('fonts/gimmefile.woff') format('woff'),
		url('fonts/gimmefile.ttf') format('truetype'),
		url('fonts/gimmefile.svg#gimmefile') format('svg')
	font-weight: normal
	font-style: normal


[class^="icon-"], [class*=" icon-"] 
	font-family: 'gimmefile'
	speak: none
	font-style: normal
	font-weight: normal
	font-variant: normal
	text-transform: none
	line-height: 1

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased
	-moz-osx-font-smoothing: grayscale


.icon-bucket:before 
	content: "\e600"

.icon-paperplane:before 
	content: "\e601"

.icon-arrow-left:before 
	content: "\e606"

.icon-arrow-down:before 
	content: "\e607"

.icon-arrow-up:before 
	content: "\e608"

.icon-arrow-right:before 
	content: "\e609"

.icon-cloud-download:before 
	content: "\e602"

.icon-cloud-upload:before 
	content: "\e603"

.icon-earth:before 
	content: "\e604"

.icon-plus:before 
	content: "\e605"

